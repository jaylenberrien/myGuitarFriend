            <input type="password" placeholder="Password" id="password-inp"/>
            <br><br>
            <button>Create Account</button>
            
        </form>
    </div>
    
    <a asp-controller="Home" asp-action="Index">go to index (temp)</a>
    <button id="testy" onclick="testFunc()">Test</button>
</div>

