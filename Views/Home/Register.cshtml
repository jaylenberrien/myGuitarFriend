@* <!DOCTYPE html>  *@
@{
    Layout = null;
}

<head>
    <link rel="stylesheet" href="/css/home/register.css"/>
</head>


@{
    ViewData["Title"] = "Register";
}

<div id="page-title">
    <h1>@ViewData["Title"]</h1>
</div>

<div id="huge-container">
    <div id="register-container">
        <form onsubmit="registerUser()">
